{
  "name": "paymenthighway-api",
  "version": "0.0.0-development",
  "description": "Salesforce paymenthighway API",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyrsky/salesforce-paymenthighway.git"
  },
  "author": "Santeri Hurnanen <santeri@oikeuttaelaimille.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hyrsky/salesforce-paymenthighway/issues"
  },
  "homepage": "https://github.com/hyrsky/salesforce-paymenthighway#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@semantic-release/exec": "^3.3.2",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^2.3.0",
    "semantic-release": "^15.13.12"
  },
  "dependencies": {},
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "./tools/prepare.sh ${nextRelease.version} ${options.branch} ${commits.length} ${Date.now()}",
          "publishCmd": "./tools/publish.sh ${nextRelease.version} ${options.branch} ${commits.length} ${Date.now()}"
        }
      ]
    ]
  }
}
